    "lodash": "^4.17.23",
    "marked": "^14.1.2",
    "ollama-ai-provider": "^1.2.0",
    "openai": "^5.20.2",
    "parse-diff": "^0.11.1",
    "pdf-parse": "^1.1.1",
    "pg": "^8.16.3",
    "prettier": "^3.6.2",
    "prom-client": "^15.1.3",
    "psl": "^1.15.0",
    "rate-limiter-flexible": "2.4.2",
    "redlock": "5.0.0-beta.2",
    "resend": "^3.5.0",
    "response-time": "^2.3.4",
