      "diff": "^8.0.3",
      "fast-xml-parser": "^5.3.4",
      "glob@>=10.2.0 <10.5.0": ">=10.5.0",
      "js-yaml@<3.14.2": ">=3.14.2",
      "qs@<6.14.1": ">=6.14.1"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,css,md}": "prettier --write"
  },
  "packageManager": "pnpm@10.16.1"
}
